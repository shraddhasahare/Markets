<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ExpandableLayout">
        <attr name="el_duration" format="integer" />
        <attr name="el_expanded" format="boolean" />
        <attr name="el_parallax" format="float" />
        <attr name="android:orientation" />
    </declare-styleable>

    <declare-styleable name="PulseIndicator">
        <attr name="minWidth" format="dimension" />
        <attr name="maxWidth" format="dimension"/>
        <attr name="minHeight" format="dimension" />
        <attr name="maxHeight" format="dimension"/>
        <attr name="indicatorName" format="string"/>
        <attr name="indicatorColor" format="color"/>
    </declare-styleable>

    <declare-styleable name="InsLoadingViewAttrr">
        <attr name="rotate_duration" format="integer" />
        <attr name="circle_duration" format="integer" />
        <attr name="start_color" format="color" />
        <attr name="end_color" format="color" />
        <attr name="LoadingStatus">
            <enum name="loading" value="0" />
            <enum name="clicked" value="1" />
            <enum name="unclicked" value="2" />
        </attr>
    </declare-styleable>

    <declare-styleable name="ScratchCardView">
        <attr name="scratchDrawable" format="reference|color" />
        <attr name="scratchWidth" format="dimension" />
    </declare-styleable>

    <!--    Custom cibil score view -->
    <declare-styleable name="CustomCibilFragView">
        <!--        //progress colors-->
        <attr name="progeress_color1" format="color" />
        <attr name="progeress_color2" format="color" />
        <attr name="progeress_color3" format="color" />

        <attr name="progress_width1" format="float" />
        <attr name="progress_width2" format="float" />
        <attr name="progress_width3" format="float" />

        <!--     inner circle-->
        <attr name="inner_circle_background" format="color" />
        <attr name="inner_circle_redius" format="float" />


        <!--     outer circle   -->
        <attr name="outer_circle_color" format="color" />
        <attr name="outer_circle_width" format="float" />
        <attr name="outer_circle_redius" format="color" />
        <attr name="is_outer_draw" format="boolean" />


        <!--      values -->
        <attr name="min" format="float" />
        <attr name="max" format="float" />
        <attr name="score" format="float" />


        <attr name="padding" format="dimension" />


        <!--        set angle of diifent arcs -->
        <attr name="starAngleOne" format="float" />
        <attr name="starAngleTwo" format="float" />
        <attr name="starAngleThree" format="float" />


        <!--        sub text -->
        <attr name="is_sub_text" format="boolean" />
        <attr name="sub_text" format="string" />
        <attr name="sub_text_color" format="color" />
        <attr name="sub_text_size" format="float" />
    </declare-styleable>

    <declare-styleable name="CustomHorizontalProgressBar">
        <attr name="cus_min" format="float" />
        <attr name="cus_max" format="float" />
        <attr name="cus_progress" format="float" />
        <attr name="cus_color" format="integer" />
    </declare-styleable>

    <declare-styleable name="StoriesProgressView">
        <attr name="progressCount" format="integer|reference" />
    </declare-styleable>

    <declare-styleable name="progressAttr">
        <attr name="sw_radius" format="dimension" />
        <attr name="sw_progressColor" format="color" />
        <attr name="sw_progressValue" format="float" />
        <attr name="sw_withAnimation" format="boolean" />
        <attr name="sw_duration" format="integer" />
        <attr name="sw_progressBackground" format="color" />
    </declare-styleable>

    <declare-styleable name="CustomProfileView">
        <attr name="inner_color" format="color" />
        <attr name="border_color" format="color" />
        <attr name="border_width" format="float" />
        <attr name="show_border" format="boolean" />
        <attr name="inner_text" format="string" />
        <attr name="view_padding" format="float" />
        <attr name="text_color" format="color" />
        <attr name="text_size" format="float" />
    </declare-styleable>
</resources>